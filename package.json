{
  "name": "angularjs-customelements-demo",
  "private": true,
  "version": "1.0.0",
  "description": "AngularJS CustomElements integration demo",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "build": "npx rollup -c",
    "watch": "npx rollup -cw",
    "serve:legacy": "npx http-server ./01-legacy -o",
    "serve:evolution-es6": "npx http-server ./02-evolution-es6 -o",
    "serve:evolution-ts": "npm run build && npx http-server ./03-evolution-ts/dist -o",
    "serve:evolution-lit": "npm run build && npx http-server ./04-evolution-lit/dist -o"
  },
  "keywords": [
    "angularjs",
    "webcomponents",
    "customelements"
  ],
  "author": "Sandro Lain",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/angular": "^1.7.2",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "css-loader": "^4.1.1",
    "electron": "^18.3.7",
    "eslint": "^7.5.0",
    "http-server": "^0.12.3",
    "jest": "^26.2.1",
    "jest-electron": "^0.1.11",
    "jest-environment-happy-dom": "0.0.43",
    "live-server": "^1.2.1",
    "rollup": "^2.23.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "to-string-loader": "^1.1.6",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "angular": "^1.8.0",
    "lit-element": "^2.3.1"
  }
}
